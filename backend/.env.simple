# Simple Configuration - No External Dependencies
FLASK_CONFIG=development
SECRET_KEY=simple-secret-key-for-demo
JWT_SECRET_KEY=simple-jwt-secret-for-demo

# SQLite Database (no MySQL needed)
DATABASE_URL=sqlite:///app.db

# Disable external services for quick start
REDIS_URL=
GOOGLE_MAPS_API_KEY=demo-key
TWILIO_ACCOUNT_SID=demo-sid
TWILIO_AUTH_TOKEN=demo-token
OPENAI_API_KEY=demo-key

# CORS for local development
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# File uploads
UPLOAD_FOLDER=uploads
MAX_CONTENT_LENGTH=16777216
